# ===========================================
# VYTARA WELLBEING APP - Environment Variables
# ===========================================
# Copy this file to .env and update with your values

# ===========================================
# API Configuration
# ===========================================
VITE_API_BASE_URL=http://localhost:3000/api
VITE_API_TIMEOUT=10000

# ===========================================
# Authentication
# ===========================================
VITE_AUTH_TOKEN_KEY=vytara_auth_token
VITE_REFRESH_TOKEN_KEY=vytara_refresh_token
VITE_USER_DATA_KEY=vytara_user_data
VITE_TOKEN_EXPIRY=900000
VITE_REFRESH_TOKEN_EXPIRY=604800000

# ===========================================
# Feature Flags
# ===========================================
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false
VITE_DEBUG_MODE=true
VITE_ENABLE_GOOGLE_AUTH=false
VITE_GOOGLE_CLIENT_ID=

# ===========================================
# Application Settings
# ===========================================
VITE_APP_ENV=development
VITE_APP_VERSION=0.0.1

# ===========================================
# AI Chat Configuration
# ===========================================
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/png,image/jpeg,image/jpg,image/gif,application/pdf

# ===========================================
# Nutrition Tracking
# ===========================================
VITE_DEFAULT_CALORIE_GOAL=2000
VITE_DEFAULT_PROTEIN_GOAL=150
VITE_DEFAULT_CARBS_GOAL=200
VITE_DEFAULT_FATS_GOAL=65
VITE_DEFAULT_WATER_GOAL=8

# ===========================================
# Task Management
# ===========================================
VITE_XP_HIGH_PRIORITY=30
VITE_XP_MEDIUM_PRIORITY=20
VITE_XP_LOW_PRIORITY=10
VITE_XP_PER_LEVEL=100

# ===========================================
# External Services (Optional)
# ===========================================
VITE_SENTRY_DSN=
VITE_GA_TRACKING_ID=
